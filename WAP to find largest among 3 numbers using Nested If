public class NestedIfPractice1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num1=2000, num2=400, num3=1500, max=0;
		
		if(num1>num2)
		{
			if(num1>num3)
			{
				max=num1;
			}
			
			else
			{
				max=num3;
			}
		}
		
		else
		{
			if(num2>num3)
			{
				max=num2;
			}
			
			else
			{
				max=num3;
			}
		}
		
	System.out.println("Largest number is "+max);	
		
	}

}
