public class Odd {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Odd obj = new Odd();
		obj.odd();

	}
	
	public void odd()
	{
		int num = 10;
		int i = 1;
		while(i<=num)
		{
			if(i<=num)
			{
				System.out.println(i);				
			}
			//System.out.println(i);
			i=i+2;
			
		}
	}

}
