public class PrimeNumber{

	public static void main(String[] args)
	{
	
		int n = 100;
		
		for(int i=2; i<n; i++)
		{
			if(n%i==0)
			{
				System.out.println("Number is not Prime");
				break;
			}
			
			else
				System.out.println("Number is Composite");
				break;
					
		}
		
		if(n<2)
		{
			System.out.println("Number is not Prime");
		}
		
		
	
	
	}

}
